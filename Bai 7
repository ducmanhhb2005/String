import java.util.*;
public class Main
{
	public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	String s= sc.nextLine();
	StringBuilder stringBuilderori=new StringBuilder(s);
	StringBuilder stringBuilderfake=stringBuilderori.reverse();
	String t=stringBuilderfake.toString();
	if (s.equals(t)) System.out.print("YES");
	else System.out.print("NO");
	}
}
